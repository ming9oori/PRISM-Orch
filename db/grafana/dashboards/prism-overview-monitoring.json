        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  agent_id as \"Agent ID\",\n  agent_type as \"Type\",\n  COUNT(*) as \"Total Tasks\",\n  SUM(CASE WHEN status = 'completed' THEN 1 ELSE 0 END) as \"Completed\",\n  SUM(CASE WHEN status = 'failed' THEN 1 ELSE 0 END) as \"Failed\",\n  ROUND(\n    (SUM(CASE WHEN status = 'completed' THEN 1 ELSE 0 END)::float / COUNT(*)) * 100, 2\n  ) as \"Success Rate %\",\n  ROUND(\n    AVG(EXTRACT(EPOCH FROM (completed_at - created_at))), 2\n  ) as \"Avg Duration\"\nFROM tasks \nWHERE created_at > NOW() - INTERVAL '24 hours'\n  AND agent_id IS NOT NULL\nGROUP BY agent_id, agent_type\nORDER BY \"Success Rate %\" DESC, \"Total Tasks\" DESC\nLIMIT 10",
          "refId": "A"
        }
      ],
