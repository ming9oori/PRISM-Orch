        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH hourly_stats AS (\n  SELECT \n    date_trunc('hour', created_at) as time,\n    status,\n    COUNT(*) as count\n  FROM tasks \n  WHERE created_at > NOW() - INTERVAL '24 hours'\n  GROUP BY date_trunc('hour', created_at), status\n  ORDER BY time\n)\nSELECT \n  extract(epoch from time) * 1000 as time,\n  COALESCE(SUM(CASE WHEN status = 'completed' THEN count END), 0) as \"Completed\",\n  COALESCE(SUM(CASE WHEN status = 'failed' THEN count END), 0) as \"Failed\",\n  COALESCE(SUM(CASE WHEN status = 'running' THEN count END), 0) as \"Running\",\n  COALESCE(SUM(CASE WHEN status = 'pending' THEN count END), 0) as \"Pending\"\nFROM hourly_stats\nGROUP BY time\nORDER BY time",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "ðŸ“Š Task Status Trends (24h)",
      "type": "timeseries"
